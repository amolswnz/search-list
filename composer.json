{
    "name": "marczhermo/search-list",
    "description": "SilverStripe SearchList",
    "type": "silverstripe-vendormodule",
    "license": "BSD-3-Clause",
    "keywords": [
        "searchlist",
        "silverstripe",
        "search",
        "admin"
    ],
    "require": {
        "silverstripe/cms": "3.6.6@stable",
        "silverstripe/framework": "3.6.6@stable"
    },
    "require-dev": {
        "phpunit/phpunit": "^3 || ^4 || ^5"
    },
    "extra": {
        "expose": [
            "client/dist",
            "client/lang"
        ],
        "branch-alias": {
            "3.x-dev": "v0.1.0"
        }
    },
    "autoload": {
        "psr-4": {
            "Marcz\\Search\\": "src/",
            "Marcz\\Search\\Tests\\": "tests/php/"
        }
    },
    "scripts": {
        "lint": "php phpcs.phar src/ tests/php/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
